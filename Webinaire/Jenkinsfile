pipeline {
    agent any
    tools {nodejs "nodejs"}
    
    stages {
      
      stage("Newman package installation") {
        steps {
          sh 'npm install -g newman'
        }
      }
      stage("Run newman") {
        steps {
          sh 'newman run Webinaire.postman_collection.json -e ProdWebinaire.postman_environment.json '
        }
      }
    }
}