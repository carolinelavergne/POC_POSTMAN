pipeline {
    agent any
    
    stages {
      stage("Npm package installation") {
        steps {
          sh 'npm install'
        }
      }
        stage("Newman package installation") {
        steps {
          sh 'npm install -g newman'
        }
      }
      stage("Run newman") {
        steps {
          sh 'newman run Newman.json -e Prod.json -d iterationData.csv'
        }
      }
    }
}
        
        
